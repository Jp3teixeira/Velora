<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.HomePageController"
            style="-fx-background-color: #4B3F72;"
            prefHeight="600.0" prefWidth="800.0">

    <StackPane AnchorPane.topAnchor="0"
               AnchorPane.bottomAnchor="0"
               AnchorPane.leftAnchor="0"
               AnchorPane.rightAnchor="0"
               alignment="CENTER">

        <Button text="LOGOUT" onAction="#handleLogout"
                style="-fx-background-color: #EAEAEA; -fx-text-fill: #4B3F72; -fx-font-weight: bold; -fx-font-size: 14px; -fx-background-radius: 15; -fx-cursor: hand; -fx-padding: 15 40;"/>
    </StackPane>
</AnchorPane>